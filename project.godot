; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=5

[application]

config/name="tomateiro"
run/main_scene="res://tomateiro/scenes/N_Bootstrap.tscn"
config/features=PackedStringArray("4.3", "Mobile")
boot_splash/bg_color=Color(0.141176, 0.141176, 0.141176, 0)
boot_splash/image="res://tomateiro/assets/imgs/gabisou-bubble-color.png"
config/icon="res://tomateiro/assets/imgs/logo_colors/tomateiro-logo-colors-icon.svg"

[autoload]

MaterialIconsDB="*res://addons/material-design-icons/icons/icons.gd"

[display]

window/size/viewport_width=640
window/size/viewport_height=360
window/size/borderless=true
window/size/transparent=true
window/per_pixel_transparency/allowed=true

[editor_plugins]

enabled=PackedStringArray("res://addons/Asset_Drawer/plugin.cfg", "res://addons/MaxSizeContainer/plugin.cfg", "res://addons/YouCanDoIt/plugin.cfg", "res://addons/material-design-icons/plugin.cfg")

[rendering]

renderer/rendering_method="gl_compatibility"
renderer/rendering_method.mobile="gl_compatibility"
environment/defaults/default_clear_color=Color(0.301961, 0.301961, 0.301961, 0)
viewport/transparent_background=true
